@model List<ResultCheffDto>
<div class="container-fluid team py-6">
    <div class="container">
        <div class="text-center wow bounceInUp" data-wow-delay="0.1s">
            <small class="d-inline-block fw-bold text-dark text-uppercase bg-light border border-primary rounded-pill px-4 py-1 mb-3">Ekibimiz</small>
            <h1 class="display-5 mb-5">Yetenkli ekibimiz ile tanışın!</h1>
        </div>
        <div class="row g-4">
            @foreach (var item in Model)
            {
                <div class="col-lg-3 col-md-6 wow bounceInUp" data-wow-delay="0.1s">
                    <div class="team-item rounded">
                        <img class="img-fluid rounded-top " src="@item.ImageURL" alt="">
                        <div class="team-content text-center py-3 bg-dark rounded-bottom">
                            <h4 class="text-primary">@item.Name</h4>
                            <p class="text-white mb-0">@item.Title</p>
                        </div>
                     
                    </div>
                </div>
            }
          
       
        </div>
    </div>
</div>