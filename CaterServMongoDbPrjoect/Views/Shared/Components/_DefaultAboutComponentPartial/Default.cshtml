@model List<ResultAboutDto>
@foreach (var item in Model)
{
    <div class="container-fluid py-6">
        <div class="container">
            <div class="row g-5 align-items-center">
                <div class="col-lg-5 wow bounceInUp" data-wow-delay="0.1s">
                    <img src="@item.ImageURL" class="img-fluid rounded" alt="">
                </div>
                <div class="col-lg-7 wow bounceInUp" data-wow-delay="0.3s">
                    <small class="d-inline-block fw-bold text-dark text-uppercase bg-light border border-primary rounded-pill px-4 py-1 mb-3">Hakkımızda</small>
                    <h1 class="display-5 mb-4">@item.Title</h1>
                    <p class="mb-4">
                        @item.Desciription
                    </p>
                    <div class="row g-4 text-dark mb-5">
                        <div class="col-sm-6">
                            <i class="fas fa-share text-primary me-2"></i>@item.Property1
                        </div>
                        <div class="col-sm-6">
                            <i class="fas fa-share text-primary me-2"></i>@item.Property2
                        </div>
                        <div class="col-sm-6">
                            <i class="fas fa-share text-primary me-2"></i>@item.Property3
                        </div>
                        <div class="col-sm-6">
                            <i class="fas fa-share text-primary me-2"></i>@item.Property4
                        </div>
                    </div>
                    <a href="" class="btn btn-primary py-3 px-5 rounded-pill">Daha Fazlası<i class="fas fa-arrow-right ps-2"></i></a>
                </div>
            </div>
        </div>
    </div>
    
}
